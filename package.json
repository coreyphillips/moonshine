{
  "name": "moonshine",
  "version": "0.2.0",
  "description": "An open-source, non-custodial, Bitcoin/Litecoin Electrum wallet for iOS & Android.",
  "author": "Corey Phillips",
  "license": "MIT",
  "homepage": "https://github.com/coreyphillips/moonshine",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coreyphillips/moonshine.git"
  },
  "bugs": {
    "url": "https://github.com/coreyphillips/moonshine/issues"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "rn-nodeify --install buffer,stream,assert,events,crypto,vm,process --hack && cd ios && pod install && cd ..",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.jsbundle --assets-dest android/app/build/intermediates/res/merged/release/ && cd android && ./gradlew assembleRelease --no-daemon"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.2",
    "@react-native-community/netinfo": "^4.2.2",
    "@react-native-community/slider": "^2.0.0",
    "assert": "^1.5.0",
    "base64-js": "^1.3.1",
    "bip21": "^2.0.2",
    "bip32": "^2.0.4",
    "bip39": "^3.0.2",
    "bitcoin-units": "^0.3.0",
    "bitcoinjs-lib": "^5.1.6",
    "bitcoinjs-message": "^2.1.0",
    "buffer": "^5.4.3",
    "crypto": "^1.0.1",
    "events": "^3.0.0",
    "lottie-ios": "3.1.3",
    "lottie-react-native": "^3.2.1",
    "moment": "^2.24.0",
    "nodejs-mobile-react-native": "^0.5.0",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "protobufjs": "^6.8.8",
    "query-string": "^6.8.3",
    "react": "^16.11.0",
    "react-native": "^0.61.4",
    "react-native-camera": "git+https://git@github.com/react-native-community/react-native-camera.git",
    "react-native-crypto": "^2.2.0",
    "react-native-keychain": "^4.0.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^11.4.0",
    "react-native-qrcode-svg": "^5.2.0",
    "react-native-randombytes": "^3.5.3",
    "react-native-snap-carousel": "^3.8.1",
    "react-native-svg": "^9.9.5",
    "react-native-touch-id": "^4.4.1",
    "react-native-typography": "^1.4.1",
    "react-native-v8": "0.61.1-patch.0",
    "react-native-vector-icons": "^6.6.0",
    "react-redux": "^7.1.1",
    "readable-stream": "^3.4.0",
    "redux": "^4.0.4",
